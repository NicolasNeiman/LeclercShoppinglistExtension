{
  "name": "Leclerc Shopping Cart Extension",
  "version": "0.1",
  "description": "Help You Get a clean list of products purchased",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "./javascript/contentscript.js" ]
    }
  ],
  "manifest_version": 2
}